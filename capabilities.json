{
  "privileges": [],
  "suppressDefaultTitle": true,

  "dataRoles": [
    { "name": "scenario", "kind": "Grouping", "displayName": "Scenario" },
    { "name": "message",   "kind": "Measure",  "displayName": "Message" },
    { "name": "detail",    "kind": "Measure",  "displayName": "Detail" },
    { "name": "severity",  "kind": "Measure",  "displayName": "Severity (optional)" },
    { "name": "value",     "kind": "Measure",  "displayName": "Value" },
    { "name": "compareTo", "kind": "Measure",  "displayName": "Compare to" }
  ],

  "dataViewMappings": [
    {
      "table": {
        "rows": {
          "select": [
            { "for": { "in": "scenario" } },
            { "for": { "in": "message" } },
            { "for": { "in": "detail" } },
            { "for": { "in": "severity" } },
            { "for": { "in": "value" } },
            { "for": { "in": "compareTo" } }
          ],
          "dataReductionAlgorithm": {
            "top": { "count": 100 }
          }
        }
      }
    }
  ],

  "objects": {
    "rule1": {
      "displayName": "Rule 1",
      "properties": {
        "enabled": {
          "displayName": "Enable rule",
          "type": { "bool": true }
        },
        "scenario": {
          "displayName": "Scenario",
          "type": { "text": true }
        },
        "operator": {
          "displayName": "Operator",
          "type": {
            "enumeration": [
              { "displayName": "Equals (=)",       "value": "eq" },
              { "displayName": "Not equal (≠)",    "value": "neq" },
              { "displayName": "Greater than (>)", "value": "gt" },
              { "displayName": "Less than (<)",    "value": "lt" }
            ]
          }
        },
        "compareSource": {
          "displayName": "Compare source",
          "type": {
            "enumeration": [
              { "displayName": "Field (CompareTo)", "value": "field" },
              { "displayName": "Fixed value",       "value": "fixed" }
            ]
          }
        },
        "fixedValue": {
          "displayName": "Fixed value",
          "type": { "numeric": true }
        },
        "trueState": {
          "displayName": "Severity when TRUE",
          "type": {
            "enumeration": [
              { "displayName": "Info",     "value": "0" },
              { "displayName": "Success",  "value": "1" },
              { "displayName": "Caution",  "value": "2" },
              { "displayName": "Critical", "value": "3" }
            ]
          }
        },
        "falseState": {
          "displayName": "Severity when FALSE",
          "type": {
            "enumeration": [
              { "displayName": "Info",     "value": "0" },
              { "displayName": "Success",  "value": "1" },
              { "displayName": "Caution",  "value": "2" },
